<h1>Listing nutrients</h1>

<table>
  <thead>
    <tr>
      <th>Nutrno</th>
      <th>Units</th>
      <th>Tagname</th>
      <th>Nutrdesc</th>
      <th>Numdec</th>
      <th>Srorder</th>
      <th>activo</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @nutrients.each do |nutrient| %>
      <tr>
        <td><%= nutrient.nutrno %></td>
        <td><%= nutrient.units %></td>
        <td><%= nutrient.tagname %></td>
        <td><%= nutrient.nutrdesc %></td>
        <td><%= nutrient.numdec %></td>
        <td><%= nutrient.srorder %></td>
        <td><%= nutrient.activo %></td>
        <td><%= link_to 'Show', nutrient %></td>
        <td><%= link_to 'Edit', edit_nutrient_path(nutrient) %></td>
        <td><%= link_to 'Destroy', nutrient, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nutrient', new_nutrient_path %>

<h2>Importar Nutrientes</h2>
<%= form_tag import_nutrients_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Importar" %>
<% end %>


